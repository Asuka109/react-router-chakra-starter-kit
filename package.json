{
  "name": "react-router-chakra-starter-kit",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "deploy": "pnpm run db:migrate:remote && pnpm run build && wrangler deploy",
    "preview": "pnpm run build && vite preview",
    "postinstall": "npm run cf-typegen",
    "cf-typegen": "wrangler types",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
    "format": "biome check --write",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply react-router-chakra-starter-kit-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply react-router-chakra-starter-kit-db --remote",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.28.0",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@react-router/fs-routes": "^7.9.5",
    "@react-router/node": "^7.9.5",
    "@react-router/serve": "^7.9.5",
    "better-auth": "^1.3.34",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.7",
    "isbot": "^5.1.31",
    "lodash": "^4.17.21",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "5.5.0",
    "react-router": "^7.9.5",
    "react-use": "^17.6.0",
    "remix-utils": "^8.8.0",
    "ufo": "^1.6.1",
    "urlpattern-polyfill": "^10.1.0",
    "vconsole": "^3.15.1"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.34",
    "@biomejs/biome": "2.3.2",
    "@cloudflare/vite-plugin": "^1.13.5",
    "@react-router/dev": "^7.9.5",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "drizzle-kit": "^0.31.6",
    "postcss": "^8.5.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.46.0"
  },
  "engines": {
    "node": ">=20.x"
  },
  "packageManager": "pnpm@10.21.0"
}
